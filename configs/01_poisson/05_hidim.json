{
    "desc": "Example hi-d delta poisson solution with bootstrapping",
    "date": "January 22, 2022",

    "rng_seed/range": [0, 100000, 1000],    
    "problem": "poisson",
    "dim/list": [16, 8],
    
    "opt/dstr": "adam",
    "opt/lr": 0.001,
    "opt/epoch": 200000,
    
    "g01/vol/n" : 400,
    "g01/srfpts/n/mdl": 1,
    "g01/srfpts/n/trg": 1,
    "g01/srfpts/dblsmpl": false,
    "g01/trg/btstrp": true,
    "g01/trg/tau": 0.999, 
    "g01/trg/reg/w": 1.0, 
    "g01/trg/w": 0.99,

    "g02/srfpts/n/mdl": 1,
    "g02/vol/n/list" : [400, 1],
    "g02/srfpts/n/trg/list": [1, 1000],
    "g02/srfpts/dblsmpl": false,
    "g02/trg/btstrp": false,

    "srfpts/detspc": false,
    
    "nn/dstr": "mlp",
    "nn/width": 64,
    "nn/hidden": 4, 
    "nn/act": "tanh", 

    "chrg/dstr": "dmm",
    "chrg/n": 1,
    "chrg/w": [1.0],
    "chrg/mu": [[0.0]],
    
    "vol/dstr": "ball",
    "vol/c/dstr": "ball",
    "vol/c/c":  [0.0],
    "vol/c/r":   1.0,
    "vol/r/dstr": "unifdpow",
    "vol/r/low":  0.0,
    "vol/r/high": 1.0,
    
    "eval/ur/dstr": "uniform",
    "eval/ur/low":  [-1.0],
    "eval/ur/high": [ 1.0],
    "eval/ur/n": 1000,
    "eval/ur/frq": 100,
    "eval/ur/store": false,
  
    "eval/ub1/dstr": "ball",
    "eval/ub1/c": [0.0],
    "eval/ub1/r": 1.0,
    "eval/ub1/n": 10000,
    "eval/ub1/frq": 1000,
    "eval/ub1/store": false,
    
    "eval/ub2/dstr": "ball",
    "eval/ub2/c": [0.0],
    "eval/ub2/r": "sqrt(dim)",
    "eval/ub2/n": 10000,
    "eval/ub2/frq": 1000,
    "eval/ub2/store": false,
    
    "eval/tv/dstr": "trnvol",
    "eval/tv/n": 10000,
    "eval/tv/frq": 1000,
    "eval/tv/store": false,
    
    "io/avg/frq":    100,
    "io/ckpt/frq":  2500,
    "io/mon/frq":   1000,
    "io/tch/dtype": "float32",
    "io/cmprssn_lvl": 0,
    "io/eval/bs": 256,
    "io/flush/frq": 0,
    
    "looping/lines": ["cart(aslist('rng_seed'),                                            ",
                      "     'dim',                                                         ",
                      "     cat(cart('g01/*').lstrip('g01/'),                              ",
                      "         cart(zip('g02/vol/n', 'g02/srfpts/n/trg'),                 ",
                      "              'g02/srfpts/n/mdl',                                   ",
                      "              'g02/srfpts/dblsmpl',                                 ",
                      "              'g02/trg/btstrp').lstrip('g02/')),                    ",
                      "     'rest')                                                        "]
  }